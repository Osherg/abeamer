{
  "config": {
    "abeamer": {
      "fps": 20,
      "width": 200,
      "height": 160,
      "version": "0.2.0",
      "locale": "en",
      "file": "__PROJDIR__/index.html",
      "renderPos": 0,
      "renderCount": 170,
      "userAgent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) HeadlessChrome/67.0.3396.62 Safari/537.36",
      "metadata": {
        "comments": [
          "My first project"
        ]
      },
      "plugins": [
        {
          "id": "abeamer.text-tasks",
          "uuid": "8b547eff-a0de-446a-9753-fb8b39d8031a",
          "author": "Alexandre Bento Freire",
          "email": "dev@a-bentofreire.com",
          "jsUrls": [
            "plugins/text-tasks/text-tasks.js"
          ],
          "teleportable": true
        }
      ],
      "animations": [
        [
          [
            {
              "selector": "#scene1-img",
              "duration": "2s",
              "props": [
                {
                  "prop": "left",
                  "value": 100
                },
                {
                  "prop": "transform",
                  "oscillator": {
                    "handler": "harmonic",
                    "params": {
                      "cycles": 2
                    }
                  },
                  "valueStart": 0,
                  "valueText": "='scaleX(' + iff(t < 0, -1, 1) + ')'"
                }
              ]
            },
            {
              "selector": "#pac-score",
              "duration": "2s",
              "props": [
                {
                  "prop": "text",
                  "easing": "easeOutQuad",
                  "valueStart": 100,
                  "valueFormat": "%d",
                  "value": 500
                }
              ]
            }
          ],
          [
            {
              "tasks": [
                {
                  "handler": "scene-transition",
                  "params": {
                    "handler": "slideLeft",
                    "duration": "1s"
                  }
                }
              ]
            }
          ]
        ],
        [
          [
            {
              "selector": "#scene2-img1",
              "duration": "2s",
              "props": [
                {
                  "prop": "left",
                  "duration": "1s",
                  "value": 133,
                  "iterationCount": 2,
                  "direction": 2
                },
                {
                  "prop": "top",
                  "duration": 1,
                  "position": "1s",
                  "value": "+5"
                },
                {
                  "prop": "src",
                  "duration": 5,
                  "iterationCount": 10,
                  "valueText": [
                    "assets/pixabay/invader-42007_640-A.png",
                    "assets/pixabay/invader-42007_640-AB.png"
                  ]
                }
              ]
            },
            {
              "selector": "#scene2-img2",
              "props": [
                {
                  "prop": "left",
                  "duration": "1s",
                  "value": 5,
                  "iterationCount": 2,
                  "direction": 2
                }
              ]
            }
          ],
          [
            {
              "tasks": [
                {
                  "handler": "scene-transition",
                  "params": {
                    "handler": "slideTop",
                    "duration": "1s"
                  }
                }
              ]
            }
          ]
        ],
        [
          [
            {
              "tasks": [
                {
                  "handler": "add-stills",
                  "params": {
                    "duration": "1s"
                  }
                }
              ]
            }
          ],
          [
            {
              "selector": "#player1",
              "duration": "1s",
              "tasks": [
                {
                  "handler": "typewriter",
                  "params": {
                    "text": "Animation 50pts"
                  }
                }
              ]
            },
            {
              "selector": "#player2",
              "duration": "1s",
              "tasks": [
                {
                  "handler": "typewriter",
                  "params": {
                    "text": "Beamer 50pts"
                  }
                }
              ]
            }
          ],
          [
            {
              "tasks": [
                {
                  "handler": "add-stills",
                  "params": {
                    "duration": "1s"
                  }
                }
              ]
            }
          ],
          [
            {
              "tasks": [
                {
                  "handler": "scene-transition",
                  "params": {
                    "handler": "slideRight",
                    "duration": "1s"
                  }
                }
              ]
            }
          ]
        ],
        [
          [
            {
              "tasks": [
                {
                  "handler": "add-stills",
                  "params": {
                    "duration": "1s"
                  }
                }
              ]
            }
          ]
        ]
      ],
      "html": [
        "",
        "        <div class=\"abeamer-scene\" id=\"scene1\">",
        "            <p>SCORE:",
        "                <span id=\"pac-score\"></span>",
        "            </p>",
        "            <img id=\"scene1-img\" src=\"assets/pixabay/pacman-148906_1280-A.png\">",
        "        </div>",
        "",
        "        <div class=\"abeamer-scene\" id=\"scene2\">",
        "            <img id=\"scene2-img1\" src=\"assets/pixabay/invader-42007_640-A.png\">",
        "            <img id=\"scene2-img2\" src=\"assets/pixabay/invader-42007_640-B.png\">",
        "        </div>",
        "",
        "        <div class=\"abeamer-scene\" id=\"scene3\">",
        "            <div id=\"board\">",
        "                <p id=\"high-score\">HIGH SCORE</p>",
        "                <p id=\"player1\"></p>",
        "                <p id=\"player2\"></p>",
        "            </div>",
        "        </div>",
        "",
        "        <div class=\"abeamer-scene\" id=\"scene4\">",
        "            <p id=\"game-over\">GAME OVER</p>",
        "            <p id=\"start-again\">1. START AGAIN</p>",
        "            <p id=\"start-again\">2. QUIT</p>",
        "        </div>",
        "    "
      ],
      "css": [
        {
          "path": "css/",
          "text": "body, html, .abeamer-story, .abeamer-scene { width: 200px; height: 160px; }"
        },
        {
          "path": "css/",
          "text": ".abeamer-story { font-family: \"Segoe UI\", Tahoma, Geneva, Verdana, sans-serif; font-size: 16px; font-weight: bold; }"
        },
        {
          "path": "css/",
          "text": "#scene1 { background: url(\"../assets/pixabay/pacman-148906_1280-B.png\") repeat rgb(51, 51, 0); color: rgb(202, 202, 202); }"
        },
        {
          "path": "css/",
          "text": "#scene1 #scene1-img { position: absolute; left: 50px; top: 40px; }"
        },
        {
          "path": "css/",
          "text": "#scene2 { background: green; color: yellow; }"
        },
        {
          "path": "css/",
          "text": "#scene2 #scene2-img1 { position: absolute; left: 20px; top: 20px; }"
        },
        {
          "path": "css/",
          "text": "#scene2 #scene2-img2 { position: absolute; left: 43px; top: 98px; }"
        },
        {
          "path": "css/",
          "text": "#scene3 { background: rgb(0, 51, 128); color: white; display: flex; width: 100%; height: 100%; align-items: center; justify-content: center; }"
        },
        {
          "path": "css/",
          "text": "#scene3 #board { border: 1px solid rgb(255, 255, 255); padding: 5px 19px; box-shadow: white 0px 0px 8px; min-height: 110px; min-width: 100px; }"
        },
        {
          "path": "css/",
          "text": "#scene3 #board #high-score { margin: 0px; border-bottom: 1px solid rgb(255, 255, 255); width: 100%; }"
        },
        {
          "path": "css/",
          "text": "#scene4 { background: rgb(87, 37, 98); color: rgb(255, 255, 255); padding: 20px; }"
        }
      ]
    }
  }
