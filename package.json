    "mkdocs-serve-developer": "cd docs/build/developer/versions/latest/en && mkdocs serve -a localhost:8100",
    "mkdocs-serve-end-user": "cd docs/build/end-user/versions/latest/en && mkdocs serve -a localhost:8200",
    "mkdocs-build-developer": "cd docs/build/developer/versions/latest/en && mkdocs build",
    "mkdocs-build-end-user": "cd docs/build/end-user/versions/latest/en && mkdocs build",
    "build-docs-and-mkdocs": "npm run build-docs && npm run mkdocs-build-developer && npm run mkdocs-build-end-user",
    "bump-version": "gulp bump-version",
    "build-release": "gulp build-release",
    "build-shared": "gulp build-shared",
    "build-docs": "gulp build-docs",
    "build-server-consts": "gulp build-server-consts",
    "gallery-update-scripts": "gulp update-gallery-scripts",
    "clean-gallery": "gulp clean-gallery",
